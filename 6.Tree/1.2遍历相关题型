//1.返回"先序遍历"序列中,第K个节点
TreeNode* getKthByPre(TreeNode* T, int Kth){
	if (Kth <= 0 || T == NULL)
		return NULL;
	//非递归先序遍历
	stack<TreeNode*> S;
	S.push(T);
	while (!S.empty()){
		TreeNode* cur = S.top(); S.pop(); 
		if (0 == --Kth){
			return cur;
		}
		if (cur->right)
			S.push(cur->right);
		if (cur->left)
			S.push(cur->left);
	}
	return NULL;
}
