
求根节点到树中某一节点node的路径（不包括node）

bool findPath(TreeNode* T, TreeNode* node, vector<TreeNode*>& path){
	if (T == node)
		return true;
	path.push_back(T);

	bool ret = false;
	if (T->left)
		ret = findPath(T->left, node, path);
	if (ret == false && T->right)
		ret = findPath(T->right, node, path);
	if (ret == false)
		path.pop_back();
	return ret;
}
